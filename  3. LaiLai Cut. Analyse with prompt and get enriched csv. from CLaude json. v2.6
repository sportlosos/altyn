ional[str]:\n","        \"\"\"–û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ—Ç –æ–¥–Ω—É —Å–µ–∫—Ü–∏—é —Ü–µ–ª–∏–∫–æ–º\"\"\"\n","        print(f\"\\n–û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–µ–∫—Ü–∏–∏ {section_index + 1}: {section['name']}\")\n","\n","        # –ù–∞—Ö–æ–¥–∏–º –≥—Ä–∞–Ω–∏—Ü—ã –≤—Å–µ–π —Å–µ–∫—Ü–∏–∏\n","        segments = section['segments']\n","        section_start = min(seg['start_time'] for seg in segments)\n","        section_end = max(seg['end_time'] for seg in segments)\n","\n","        print(f\"–ì—Ä–∞–Ω–∏—Ü—ã —Å–µ–∫—Ü–∏–∏: {section_start:.2f} -\u003e {section_end:.2f}\")\n","        print(f\"–û–±—â–∞—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å: {section_end - section_start:.2f} —Å–µ–∫\")\n","\n","        # –°–æ–∑–¥–∞–µ–º –∏–º—è —Ñ–∞–π–ª–∞ –¥–ª—è —Å–µ–∫—Ü–∏–∏\n","        safe_name = f\"section_{section_index + 1:02d}_{section['name'].replace(' ', '_')}\"\n","        output_file = os.path.join(self.sections_dir, f\"{safe_name}.mp4\")\n","\n","        # –°–æ–±–∏—Ä–∞–µ–º —Ñ—Ä–∞–≥–º–µ–Ω—Ç—ã –¥–ª—è –≤—ã—Ä–µ–∑–∞–Ω–∏—è (type='cut')\n","        cuts = [seg for seg in segments if seg['type'] == 'cut']\n","\n","        if not cuts:\n","            # –ï—Å–ª–∏ –Ω–µ—Ç –≤—ã—Ä–µ–∑–æ–∫, –±–µ—Ä–µ–º —Å–µ–∫—Ü–∏—é —Ü–µ–ª–∏–∫–æ–º\n","            print(\"–°–µ–∫—Ü–∏—è –±–µ–∑ –≤—ã—Ä–µ–∑–æ–∫, –∏–∑–≤–ª–µ–∫–∞–µ–º —Ü–µ–ª–∏–∫–æ–º\")\n","            self.extract_segment(\n","                input_video,\n","                output_file,\n","                section_start,\n","                section_end,\n","                f\"–°–µ–∫—Ü–∏—è {section['name']}\"\n","            )\n","        else:\n","            print(f\"–ù–∞–π–¥–µ–Ω–æ {len(cuts)} —Ñ—Ä–∞–≥–º–µ–Ω—Ç–æ–≤ –¥–ª—è –≤—ã—Ä–µ–∑–∞–Ω–∏—è:\")\n","            for cut in cuts:\n","                print(f\"  - {cut['description']}: {cut['start_time']:.2f} -\u003e {cut['end_time']:.2f}\")\n","\n","            # –°–æ–∑–¥–∞–µ–º —Å–ø–∏—Å–æ–∫ —Ñ—Ä–∞–≥–º–µ–Ω—Ç–æ–≤ –¥–ª—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è\n","            keep_segments = []\n","            current_time = section_start\n","\n","            # –°–æ—Ä—Ç–∏—Ä—É–µ–º –≤—ã—Ä–µ–∑–∫–∏ –ø–æ –≤—Ä–µ–º–µ–Ω–∏\n","            cuts.sort(key=lambda x: x['start_time'])\n","\n","            # –°–æ–∑–¥–∞–µ–º —Å–µ–≥–º–µ–Ω—Ç—ã –º–µ–∂–¥—É –≤—ã—Ä–µ–∑–∫–∞–º–∏\n","            for cut in cuts:\n","                if cut['start_time'] \u003e current_time:\n","                    keep_segments.append({\n","                        'start': current_time,\n","                        'end': cut['start_time']\n","                    })\n","                current_time = cut['end_time']\n","\n","            # –î–æ–±–∞–≤–ª—è–µ–º –ø–æ—Å–ª–µ–¥–Ω–∏–π —Å–µ–≥–º–µ–Ω—Ç, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ\n","            if current_time \u003c section_end:\n","                keep_segments.append({\n","                    'start': current_time,\n","                    'end': section_end\n","                })\n","\n","            if not keep_segments:\n","                print(\"–ü—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–µ: –≤—Å—è —Å–µ–∫—Ü–∏—è –ø–æ–º–µ—á–µ–Ω–∞ –¥–ª—è –≤—ã—Ä–µ–∑–∞–Ω–∏—è\")\n","                return None\n","\n","            # –°–æ–∑–¥–∞–µ–º –≤—Ä–µ–º–µ–Ω–Ω—ã–µ —Ñ–∞–π–ª—ã –¥–ª—è —Å–æ—Ö—Ä–∞–Ω—è–µ–º—ã—Ö —á–∞—Å—Ç–µ–π\n","            temp_files = []\n","            for i, segment in enumerate(keep_segments):\n","                temp_file = os.path.join(self.sections_dir, f'temp_{section_index}_{i}.mp4')\n","                self.extract_segment(\n","                    input_video,\n","                    temp_file,\n","                    segment['start'],\n","                    segment['end'],\n","                    f\"–ß–∞—Å—Ç—å {i+1} —Å–µ–∫—Ü–∏–∏ {section['name']}\"\n","                )\n","                temp_files.append(temp_file)\n","\n","            # –°–∫–ª–µ–∏–≤–∞–µ–º —Å–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã–µ —á–∞—Å—Ç–∏\n","            self.concat_segments(temp_files, output_file)\n","\n","            # –û—á–∏—â–∞–µ–º –≤—Ä–µ–º–µ–Ω–Ω—ã–µ —Ñ–∞–π–ª—ã\n","            for temp_file in temp_files:\n","                if os.path.exists(temp_file):\n","                    os.remove(