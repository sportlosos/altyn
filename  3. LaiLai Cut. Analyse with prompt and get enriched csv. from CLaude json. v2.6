[0m in \u001b[0;36m_wait\u001b[0;34m(self, timeout)\u001b[0m\n\u001b[1;32m   1951\u001b[0m                         \u001b[0;32mraise\u001b[0m \u001b[0mTimeoutExpired\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mself\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0margs\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mtimeout\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m   1952\u001b[0m                     \u001b[0mdelay\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mmin\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mdelay\u001b[0m \u001b[0;34m*\u001b[0m \u001b[0;36m2\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mremaining\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;36m.05\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m-\u003e 1953\u001b[0;31m                     \u001b[0mtime\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0msleep\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mdelay\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m   1954\u001b[0m             \u001b[0;32melse\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m   1955\u001b[0m                 \u001b[0;32mwhile\u001b[0m \u001b[0mself\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mreturncode\u001b[0m \u001b[0;32mis\u001b[0m \u001b[0;32mNone\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;31mKeyboardInterrupt\u001b[0m: "]}],"source":["import os\n","import pandas as pd\n","import subprocess\n","import json\n","import re\n","from pathlib import Path\n","import logging\n","from typing import List, Dict, Optional\n","\n","# –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –ª–æ–≥–∏—Ä–æ–≤–∞–Ω–∏—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è –≤—Å–µ—Ö —ç—Ç–∞–ø–æ–≤ –æ–±—Ä–∞–±–æ—Ç–∫–∏\n","logging.basicConfig(\n","    level=logging.INFO,\n","    format='%(asctime)s - %(levelname)s - %(message)s'\n",")\n","\n","class VideoProcessor:\n","    def __init__(self, video_path: str, csv_path: str, output_dir: str):\n","        \"\"\"\n","        –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø—Ä–æ—Ü–µ—Å—Å–æ—Ä–∞ –≤–∏–¥–µ–æ.\n","\n","        Args:\n","            video_path (str): –ü—É—Ç—å –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É –≤–∏–¥–µ–æ\n","            csv_path (str): –ü—É—Ç—å –∫ CSV —Ñ–∞–π–ª—É —Å —Ä–∞–∑–º–µ—Ç–∫–æ–π\n","            output_dir (str): –ü—É—Ç—å –∫ –¥–∏—Ä–µ–∫—Ç–æ—Ä–∏–∏ –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤\n","        \"\"\"\n","        self.video_path = Path(video_path)\n","        self.csv_path = Path(csv_path)\n","        self.output_dir = Path(output_dir)\n","        self.output_dir.mkdir(parents=True, exist_ok=True)\n","\n","        # –ü–æ–ª—É—á–∞–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –≤–∏–¥–µ–æ –ø—Ä–∏ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏\n","        self.video_info = self._get_video_info()\n","        logging.info(f\"–ó–∞–≥—Ä—É–∂–µ–Ω–æ –≤–∏–¥–µ–æ: {self.video_path}\")\n","        logging.info(f\"FPS: {self.video_info['fps']}, \"\n","                    f\"–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å: {self.video_info['duration']}, \"\n","                    f\"–†–∞–∑—Ä–µ—à–µ–Ω–∏–µ: {self.video_info['width']}x{self.video_info['height']}\")\n","\n","        # –ó–∞–≥—Ä—É–∂–∞–µ–º CSV\n","        logging.info(\"Loading and processing CSV file...\")\n","        self.df = self._load_csv()\n","\n","    def _get_video_info(self) -\u003e dict:\n","        \"\"\"–ü–æ–ª—É—á–µ–Ω–∏–µ –¥–µ—Ç–∞–ª—å–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –≤–∏–¥–µ–æ —Ñ–∞–π–ª–µ —á–µ—Ä–µ–∑ ffprobe\"\"\"\n","        try:\n","            command = [\n","                'ffprobe',\n","                '-v', 'quiet',\n","                '-print_format', 'json',\n","                '-show_format',\n","                '-show_streams',\n","                str(self.video_path)\n","            ]\n","\n","            result = subprocess.run(command, capture_output=True, text=True, check=True)\n","            info = json.loads(result.stdout)\n","\n","            # –ò—â–µ–º –≤–∏–¥–µ–æ –ø–æ—Ç–æ–∫\n","            video_stream = next(\n","                (stream for stream in info['streams'] if stream['codec_type'] == 'video'),\n","                None\n","            )\n","\n","            if not video_stream:\n","                raise ValueError(\"–í–∏–¥–µ–æ –ø–æ—Ç–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω\")\n","\n","            # –ü–æ–ª—É—á–∞–µ–º –∏ –ø—Ä–æ–≤–µ—Ä—è–µ–º FPS\n","            fps_str = video_stream.get('r_frame_rate', '0/0')\n","            num, den = map(int, fps_str.split('/'))\n","            fps = num / den if den != 0 else 0\n","\n","            if fps == 0:\n","                raise ValueError(\"–ù–µ–∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ FPS\")\n","\n","            return {\n","                'fps': fps,\n","                'duration': float(info['format'].get('duration', 0)),\n","                'frame_count': int(video_stream.get('nb_frames', 0)),\n","                'codec_name': video_stream.get('codec_name', ''),\n","                'width': int(video_stream.get('width', 0)),\n","                'height': int(video_stream.get('height', 0))\n","            }\n","\n","        except Exception as e:\n","            logging.error(f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –ø–æ–ª—É—á–µ–Ω–∏–∏ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –≤–∏–¥–µ–æ: {str(e)}\")\n","            raise\n","\n","    def _load_csv(self) -\u003e pd.DataFrame:\n","        \"\"\"–ó–∞–≥—Ä—É–∑–∫–∞ –∏ –æ–±—Ä–∞–±–æ—Ç–∫–∞ CSV —Ñ–∞–π–ª–∞ —Å –ø—Ä–µ–¥–≤–∞—Ä–∏—Ç–µ–ª—å–Ω–æ–π –æ–±—Ä–∞–±–æ—Ç–∫–æ–π NaN\"\"\"\n","        try:\n","            # –ß–∏—Ç–∞–µ–º CSV —Ñ–∞–π–ª\n","            df = pd.read_csv(self.csv_path)\n","\n","            # –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º NaN –≤ —Ç–µ–∫—Å—Ç–æ–≤—ã—Ö —Å—Ç–æ–ª–±—Ü–∞—Ö\n","            text_columns = ['section_name']\n","            for col in text_columns:\n","                df[col] = df[col].fillna('Unknown_Section')\n","\n","            # –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º NaN –≤ —á–∏—Å–ª–æ–≤—ã—Ö —Å—Ç–æ–ª–±—Ü–∞—Ö\n","            numeric_pairs = [\n","                ('sentence_start', 'section_start'),\n","                ('sentence_end', 'section_end')\n","            ]\n","\n","            for target_col, source_col in numeric_pairs:\n","                df[target_col] = df[target_col].fillna(df[source_col])\n","\n","            # –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º —á–∏—Å–ª–æ–≤—ã–µ —Å—Ç–æ–ª–±—Ü—ã –∏ –ø—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞ –æ—Å—Ç–∞–≤—à–∏–µ—Å—è NaN\n","            numeric_columns = ['section_start', 'section_end', 'sentence_start', 'sentence_end']\n","            for col in numeric_columns:\n","                df[col] = pd.to_numeric(df[col], errors='coerce')\n","                if col.endswith('_start'):\n","                    df[col] = df[col].fillna(0)\n","                else:  # –¥–ª—è —Å—Ç–æ–ª–±—Ü–æ–≤ _end\n","                    max_time = max(df[col].max() or 0,\n","                                 df['section_end'].max() or 0,\n","                                 float(self.video_info['duration']))\n","                    df[col] = df[col].fillna(max_time)\n","\n","            logging.info(\"CSV –¥–∞–Ω–Ω—ã–µ —É—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω—ã –∏ –æ–±—Ä–∞–±–æ—Ç–∞–Ω—ã\")\n","            logging.info(\"\\n–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –æ–±—Ä–∞–±–æ—Ç–∞–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö:\")\n","            logging.info(f\"–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç—Ä–æ–∫: {len(df)}\")\n","            logging.info(f\"–ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ NaN:\\n{df.isna().sum()}\")\n","\n","            return df\n","\n","        except Exception as e:\n","            logging.error(f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ —á—Ç–µ–Ω–∏–∏ CSV: {str(e)}\")\n","            raise\n","\n","    def _sanitize_filename(self, name: str) -\u003e str:\n","        \"\"\"–û—á–∏—Å—Ç–∫–∞ –∏–º–µ–Ω–∏ —Ñ–∞–π–ª–∞ –æ—Ç –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º—ã—Ö —Å–∏–º–≤–æ–ª–æ–≤\"\"\"\n","        name = name.replace('—å', '').replace('—ä', '')\n","        name = re.sub(r'[^\\w\\-\\.]', '_', name)\n","        name = re.sub(r'_+', '_', name)\n","        return name.strip('_')\n","\n","    def _align_timestamp_to_keyframe(self, timestamp: float) -\u003e float:\n","        \"\"\"–í—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–π –º–µ—Ç–∫–∏ –ø–æ –±–ª–∏–∂–∞–π—à–µ–º—É –∫–µ–π—Ñ—Ä–µ–π–º—É\"\"\"\n","        frame_time = 1 / self.video_info['fps']\n","        frame_number = round(timestamp / frame_time)\n","        return frame_number * frame_time\n","\n","    def _extract_segment(self, start_time: float, end_time: float, output_path: str) -\u003e None:\n","        \"\"\"–ò–∑–≤–ª–µ—á–µ–Ω–∏–µ —Å–µ–≥–º–µ–Ω—Ç–∞ –≤–∏–¥–µ–æ —Å –≤—ã—Å–æ–∫–∏–º –∫–∞—á–µ—Å—Ç–≤–æ–º –∏ —É—á–µ—Ç–æ–º FPS\"\"\"\n","        try:\n","            # –í—ã—Ä–∞–≤–Ω–∏–≤–∞–µ–º –≤—Ä–µ–º–µ–Ω–Ω—ã–µ –º–µ—Ç–∫–∏ –ø–æ —Ñ—Ä–µ–π–º–∞–º\n","            aligned_start = self._align_timestamp_to_keyframe(start_time)\n","            aligned_end = self._align_timestamp_to_keyframe(end_time)\n","            duration = aligned_end - aligned_start\n","\n","            # –†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è –≤—ã—Å–æ–∫–æ–≥–æ –∫–∞—á–µ—Å—Ç–≤–∞\n","            command = [\n","                'ffmpeg',\n","                '-ss', f\"{aligned_start:.3f}\",\n","                '-i', str(self.video_path),\n","                '-t', f\"{duration:.3f}\",\n","                '-c:v', 'libx264',\n","                '-preset', 'slow',\n","                '-crf', '18',\n","                '-c:a', 'aac',\n","                '-b:a', '192k',\n","                '-vsync', 'cfr',\n","                '-r', f\"{self.video_info['fps']}\",\n","                '-movflags', '+faststart',\n","                '-y',\n","                output_path\n","            ]\n","\n","            logging.info(f\"–ò–∑–≤–ª–µ—á–µ–Ω–∏–µ —Å–µ–≥–º–µ–Ω—Ç–∞: {aligned_start:.3f} - {aligned_end:.3f}\")\n","            logging.info(f\"FPS: {self.video_info['fps']}, Duration: {duration:.3f}\")\n","            subprocess.run(command, check=True, capture_output=True)\n","\n","        except subprocess.CalledProcessError as e:\n","            logging.error(f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –∏–∑–≤–ª–µ—á–µ–Ω–∏–∏ —Å–µ–≥–º–µ–Ω—Ç–∞: {e.stderr}\")\n","            raise\n","\n","    def _concat_videos(self, input_files: List[str], output_path: str) -\u003e None:\n","        \"\